NATIVE_ENABLED = true
DEBUG=-g
WARN_ERROR=-warn-error a
OCAMLFLAGS    += $(DEBUG) $(WARN_ERROR)
OCAMLOPTFLAGS = $(DEBUG) # -inline 100
USE_OCAMLFIND = true

OCAMLPACKS[] =
	batteries
	tokyo_cabinet
	json-wheel
	getopt

OBJECTS[] =
	h
	utils
	graph
	common
	t
	constants
	json_graph
	tokyo_graph
	binary_graph
	invert
	by_day
	dranges
	dreps
	proportional
	simulate
	skew
	soc_run_common
	soc_run_gen
	sg
	
OCamlProgram(sg, $(OBJECTS))

.DEFAULT: sg

.PHONY: clean

clean:
	rm -f $(filter-proper-targets $(ls R, .)) *.s
